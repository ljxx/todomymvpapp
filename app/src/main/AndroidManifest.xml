<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ylx.todomvpapp">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="我是首页"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.ToolbarActivity"
            android:label="我是toolbar设置页" />
        <activity android:name=".ui.activity.CallPhoneActivity" />
        <activity
            android:name=".ui.activity.DrawerLayoutActivity"
            android:label="我是滑动页面" />
        <activity android:name=".ui.activity.CardViewActivity" android:label="卡片式列表页"></activity>
    </application>

</manifest>